{
  "name": "bs_frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build hui": "vite build",
    "build": "npm run build:css",
    "build:css": "NODE_ENV=production tailwindcss -o src/assets/css/output.css -m",
    "start": "npm run start:css & npm run dev",
    "start:css": "tailwindcss -o src/assets/css/output.css --watch",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.8",
    "flowbite": "^2.3.0",
    "flowbite-vue": "^0.1.5",
    "laravel-vue-pagination": "^4.1.3",
    "pinia": "^2.1.7",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "laravel-echo": "^1.16.1",
    "prettier": "^3.0.3",
    "pusher-js": "^8.4.0-rc2",
    "sass": "^1.71.1",
    "tailwindcss-theme-variants": "^2.0.0-beta.0",
    "vite": "^5.0.11",
    "vitest": "^1.6.0"
  }
}
